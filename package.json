  "scripts": {
    "bin:start": "NODE_ENV=production PORT=8080 APIPORT=3030 node --trace-deprecation bin/start.js",
    "run:dev": "npm-run-all dev:prepare development",
    "dev:prepare": "universal-webpack --settings ./webpack/universal-webpack-settings.json prepare",
    "development": "NODE_ENV=development npm-run-all --parallel dev:client:build dev:server:build start:dev",
    "dev:server": "NODE_ENV=development WEBPACK_DLLS=1 PORT=3000 APIPORT=3030 nodemon ./bin/server.js --watch ./server --watch ./build/server --no-update-notifier",
    "dev:api": "NODE_ENV=development APIPORT=3030 nodemon ./bin/api.js --watch ./api --no-update-notifier",
    "dev:client:build": "NODE_PATH=client NODE_ENV=development WEBPACK_DLLS=1 PORT=3000 APIPORT=3030 node ./webpack/webpack.dev.server.js",
    "dev:server:build": "NODE_ENV=development webpack --mode development --config ./webpack/webpack.config.server.development.babel.js --watch --display-error-details",
    "start:dev": "concurrently \"yarn dev:server\" \"yarn dev:api\"",
    "start:prod": "concurrently \"yarn prod:server\" \"yarn prod:api\"",
    "prod:server": "NODE_ENV=production BABEL_DISABLE_CACHE=1 PORT=8080 APIPORT=3030 node ./bin/server.js",
    "prod:api": "NODE_ENV=production APIPORT=3030 node ./bin/api.js",
    "prod:build:client:server": "concurrently \"yarn prod:client:build\" \"yarn prod:server:build\"",
    "prod:client:build": "NODE_ENV=production webpack --mode production --display-error-details --config ./webpack/client.prod.js",
    "prod:server:build": "NODE_ENV=production webpack --mode production --display-error-details --config ./webpack/server.prod.js",
    "prod:build:stats": "NODE_ENV=production webpack --profile --json > stats.json --mode production --config ./webpack/client.prod.js",
    "dlls": "webpack --display-error-details --config webpack/dllplugin.vendor.config.js",
    "prettier": "prettier-eslint --write --eslint-config-path .eslintrc './+(bin)/**/*.js'",
    "lint": "eslint -c .eslintrc bin && flow",
    "test": "NODE_PATH=client jest client"
  },
